{
  "name": "realpolitik",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "worker": "python worker/run_local.py",
    "worker:watch": "python worker/run_local.py --watch",
    "worker:watch:fast": "python worker/run_local.py --watch --interval 2",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:worker": "cd worker && python -m pytest",
    "test:worker:cov": "cd worker && python -m pytest --cov=. --cov-report=term-missing",
    "test:all": "npm run test:run && npm run test:worker"
  },
  "dependencies": {
    "@google/genai": "^1.35.0",
    "@supabase/supabase-js": "^2.93.1",
    "@tailwindcss/typography": "^0.5.19",
    "@tavily/core": "^0.6.4",
    "@upstash/redis": "^1.36.1",
    "@vercel/analytics": "^1.6.1",
    "framer-motion": "^12.25.0",
    "mapbox-gl": "^3.17.0",
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-map-gl": "^8.1.0",
    "react-markdown": "^10.1.0",
    "swr": "^2.3.8",
    "web-push": "^3.6.7"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/web-push": "^3.6.4",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jsdom": "^24.0.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "supabase": "^2.72.8",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^1.2.0"
  }
}
